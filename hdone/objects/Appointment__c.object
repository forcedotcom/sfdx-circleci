    <validationRules>
        <fullName>AppointmentTimeLessThanFiveMinutes</fullName>
        <active>true</active>
        <description>Checks that Appointment is at least 5 minutes</description>
        <errorConditionFormula>(End_Time__c -  Start_Time__c) &lt; 5/1440</errorConditionFormula>
        <errorDisplayField>End_Time__c</errorDisplayField>
        <errorMessage>Appointments must be at least 5 minutes. Please check the values in the fields</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EndTimeBlank</fullName>
        <active>true</active>
        <description>Checks if type of appointment is Manual and End Time has no value</description>
        <errorConditionFormula>AND(ISPICKVAL(Appointment_Type__r.Type__c , &apos;Manual&apos;),  ISBLANK( End_Time__c ) )</errorConditionFormula>
        <errorDisplayField>End_Time__c</errorDisplayField>
        <errorMessage>Because the type of Appointment is Manual a value must be entered in this field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EndTimeGreaterThanScheduleEnd</fullName>
        <active>true</active>
        <errorConditionFormula>End_Time__c &gt;  Schedule__r.EndDateTime__c</errorConditionFormula>
        <errorDisplayField>End_Time__c</errorDisplayField>
        <errorMessage>Appointment time outside of window</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EndTimeInPast</fullName>
        <active>true</active>
        <description>Checks to ensure End Time not entered in past unless retroactive appointment.</description>
        <errorConditionFormula>AND( Retroactive__c = False,   End_Time__c &lt; NOW())</errorConditionFormula>
        <errorDisplayField>End_Time__c</errorDisplayField>
        <errorMessage>The End Time cannot be in the past unless this is a retroactive appointment. Please change the information as needed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>StartTimeInPast</fullName>
        <active>true</active>
        <description>Checks to ensure Start Time not entered in past unless retroactive appointment.</description>
        <errorConditionFormula>AND( Retroactive__c = False,  Start_Time__c &lt; NOW())</errorConditionFormula>
        <errorDisplayField>Start_Time__c</errorDisplayField>
        <errorMessage>The Start Time cannot be in the past unless this is a retroactive appointment. Please change the information as needed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>StartTimeLessThanScheduleStart</fullName>
        <active>true</active>
        <errorConditionFormula>Start_Time__c &lt; Schedule__r.StartDateTime__c</errorConditionFormula>
        <errorDisplayField>Start_Time__c</errorDisplayField>
        <errorMessage>Appointment time outside of window</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>StatusBlank</fullName>
        <active>true</active>
        <description>Ensures that a value for Status is picked</description>
        <errorConditionFormula>ISBLANK(TEXT( Status__c ))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Please select an Appointment status</errorMessage>
    </validationRules>
