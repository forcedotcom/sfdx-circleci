    <validationRules>
        <fullName>AppointmentTypeManual</fullName>
        <active>true</active>
        <description>Checks if Type field is Manual</description>
        <errorConditionFormula>AND(ISPICKVAL( Type__c , &apos;Manual&apos;),  (Number_of_Minutes__c &lt;&gt; 0), OR( Number_of_Minutes__c &gt; 0 , NOT(ISBLANK(Number_of_Minutes__c ))))

/*Checks to see if Type field is Manual and if a value in Number of Minutes */</errorConditionFormula>
        <errorDisplayField>Number_of_Minutes__c</errorDisplayField>
        <errorMessage>Predefined values cannot be entered for Manual appointments. Appointment Start and End times are manually entered for these. Please remove the value in this field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AppointmentTypeNotManual</fullName>
        <active>true</active>
        <description>Checks Appointment Type to ensure values entered into Number of Minutes when not Manual.</description>
        <errorConditionFormula>AND(NOT(ISPICKVAL( Type__c , &apos;Manual&apos;)),  OR(Number_of_Minutes__c &lt; 5,Number_of_Minutes__c &gt;240, ISBLANK(Number_of_Minutes__c )))

/*Checks to see if Type field is not Manual and if no value in Number of Minutes */</errorConditionFormula>
        <errorDisplayField>Number_of_Minutes__c</errorDisplayField>
        <errorMessage>Unless the type of appointment is Manual then appointments must be at least 5 minutes and less than 240 minutes</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LocationChanged</fullName>
        <active>true</active>
        <description>Checks if location is changed on Appointment Type. Do not want this. Should create a new one instead.</description>
        <errorConditionFormula>ISCHANGED( Address_Type__c )</errorConditionFormula>
        <errorDisplayField>Address_Type__c</errorDisplayField>
        <errorMessage>Locations cannot be changed on these records. Please create a new record instead if you want to assign values to a different location.</errorMessage>
    </validationRules>
